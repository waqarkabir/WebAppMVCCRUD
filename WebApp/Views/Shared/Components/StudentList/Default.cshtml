@model IEnumerable<Student>
@{
    Layout = null;
}
<div class="">
    <table class="table">
        <thead>
            <tr>
                <th>Name</th>
                <th>Cnic</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>

            @foreach (var student in Model)
            {
                <tr>
                    <td>@student.Name</td>
                    <td>@student.Cnic</td>
                    <td>
                        <a asp-controller="Student" asp-action="Update" class="btn btn-sm btn-info js-edit" asp-route-id=@student.Id>Edit</a>
                        <a href="#" class="btn btn-sm btn-danger js-delete" data-id=@student.Id>Delete</a>
                        <a asp-controller="Student" asp-action="Detail" class="btn btn-sm btn-primary js-detail" asp-route-id=@student.Id>Details</a>

                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>